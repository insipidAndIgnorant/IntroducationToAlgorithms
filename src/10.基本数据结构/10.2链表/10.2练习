10.2-7
  var reverseList = function(head) {
    if (head === null) {
        return head
    }
    let first = head, target = head.next;
    while (target) {
        first.next = target.next;
        target.next = head;
        head = target;
        target =first.next;
       
    }

    return head
  };
  // 与letcode上相同

